
<h1 class="title">Log In</h1>

<div class="section">
    <%= form_tag '/login' do %>

    <div class="field">
        <div class="control">
            <%= label_tag :email %>
            <%= text_field_tag :email %>
            
        </div>
    </div>
    <div class="field">
        <div class="control">
   <%= label_tag :password%>
   <%= password_field_tag :password%>
        </div>
    </div>
   <%= submit_tag "Log In"%>

    <% end %>
</div>


